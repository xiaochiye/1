#include "stdio.h"
#include "string.h"

int main()
{
	char strExp[]="1-2.5*4+10.2/5.1";
	char strTmp[100];
	int strTmpIndex=-1;


	for(int i=0;i<strlen(strExp);i++)
	{
		if(strExp[i]=='*')
		{
			int l=strTmp[strTmpIndex]-'0';
			int r=strExp[i+1]-'0';
            strTmp[strTmpIndex]=l*r+'0';
			i++;
		}
		else if(strExp[i]=='/')
		{
			int l=strTmp[strTmpIndex]-'0';
			int r=strExp[i+1]-'0';
            strTmp[strTmpIndex]=l/r+'0';
			i++;
		}
		else strTmp[strTmpIndex]=strExp[i];
	}

	int res=strTmp[0]-'0',a=1;
	for(a;a<strlen(strTmp);a++)
		if(strTmp[a]=='+')
	{
		{
			res+=strTmp[++a]-'0';
		}
		else if(strTmp[a]=='-')
		{
			res-=strTmp[++a]-'0';
		}
	}

	printf("%d\n\n",res);
	return 0;
}
