#include "stdio.h"
#include "string.h"
int main()
{

	char pu[]="2+2*3+2/2-1";
	int v3=2;
	
	for(i=0;i<strlen(pu);i++)
	{
		if(pu[i]=='+')
		{
			if(pu[i+2]=='*')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3+l*r;
			}
			else if(pu[i+2]=='/')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3+l/r;
			}
			else if(pu[i+2]=='\0')
			{
				int r=pu[i+1]-'0';
				v3=v3+r;
			}
		}
		else if(pu[i]=='-')
		{
			if(pu[i+2]=='*')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3-l*r;
			}
			else if(pu[i+2]=='/')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3-l/r;
			}
			else if(pu[i+2]=='\0')
			{
				int r=pu[i+1]-'0';
				v3=v3-r;
			}
		}
	}
	
	printf("v3=%d\n\n",v3);
	
	
	return 0;
}
